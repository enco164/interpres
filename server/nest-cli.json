{
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/gateway/src",
  "monorepo": true,
  "root": "apps/gateway",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/gateway/tsconfig.app.json"
  },
  "projects": {
    "gateway": {
      "type": "application",
      "root": "apps/gateway",
      "entryFile": "main",
      "sourceRoot": "apps/gateway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/gateway/tsconfig.app.json"
      }
    },
    "translations": {
      "type": "application",
      "root": "apps/translations",
      "entryFile": "main",
      "sourceRoot": "apps/translations/src",
      "compilerOptions": {
        "tsConfigPath": "apps/translations/tsconfig.app.json"
      }
    },
    "transfer": {
      "type": "application",
      "root": "apps/transfer",
      "entryFile": "main",
      "sourceRoot": "apps/transfer/src",
      "compilerOptions": {
        "tsConfigPath": "apps/transfer/tsconfig.app.json"
      }
    },
    "integration": {
      "type": "application",
      "root": "apps/integration",
      "entryFile": "main",
      "sourceRoot": "apps/integration/src",
      "compilerOptions": {
        "tsConfigPath": "apps/integration/tsconfig.app.json"
      }
    }
  }
}